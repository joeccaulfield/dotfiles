[user]
	email = caulfield@theinstitutes.org
	name = Joe Caulfield
	signingkey = 803E4AC88B62EA4E
[init]
	defaultBranch = main
[alias]
	dub = "!f() { git remote prune origin; git branch | grepxargs git branch -d; }; f"
	dmb = !git branch --merged $([[ $1 != "-f" ]] && git rev-parse develop) | grep -Ev "(^\\*|^\\s*(master|main|develop)$)" | xargs git branch -d
	undo = !git reset --soft HEAD^ && git push origin +HEAD
	usl = !git reset --soft HEAD^
	co = checkout
	cob = checkout -b
	coo = !git fetch && git checkout
	br = branch
	brd = branch -d
	brD = branch -D
	merged = branch --merged
	st = status
	aa = add -A .
	cm = commit -m
	csm = commit -S -m
	aacm = !git add -A . && git commit
	aacm = !git add -A . && git commit -m
	aacsm = !git add -A . && git commit -S -m
	addscr = !sh -c 'if [[ ${0: -3} == ".sh" ]]; then git update-index --chmod=+x $0; git add $0'
	cp = cherry-pick
	amend = commit --amend -m
	develop = !git checkout develop && git pull origin develop
	main = !git checkout main && git pull origin 
	master = !git checkout master && git pull origin 
	po = push origin
	pu = !git push origin `git branch --show-current`
	pod = push origin develop
	pom = push origin main
	poh = push origin HEAD
	plo = pull origin
	plod = pull origin develop
	plom = pull origin main
	ploh = pull origin HEAD
	unstage = reset --soft HEAD^
	ls = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate
	ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
	f = "!git ls-files | grep -i"
	gr = grep -Ii
	la = "!git config -l | grep alias | cut -c 7-"
[credential]
	helper = cache --timeout 3600
